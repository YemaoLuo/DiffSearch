-    // some are not used
-    private static final Map map = new HashMap(); // Map<String,Pattern>
-    private InstructionList il;
-    private String il_string; // instruction list
-    // as string
-    private InstructionHandle[] handles; // map instruction
-
-
-    // list to array
-    /**
-     * @param il
